{
  "name": "DetoxTest",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "clean": "npm start -- --reset-cache",
    "ios": "react-native run-ios",
    "linter": "eslint --fix 'src/**/*.{js,jsx,ts,tsx}'",
    "prepare": "husky install",
    "prettier": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
    "sort-package-json": "npx sort-package-json",
    "start": "react-native start",
    "test": "jest",
    "generate": "plop && prettier --write 'src/**/*.{js,jsx,ts,tsx}' && eslint --fix 'src/**/index.ts'",
    "detox:ios-debug": "detox build --configuration ios.sim.debug",
    "detox:e2e:ios-debug": "DETOX_CONFIGURATION=ios.sim.debug npx cucumber-js",
    "detox:ios-release": "detox build --configuration ios.sim.release",
    "detox:e2e:ios-release": "DETOX_CONFIGURATION=ios.sim.release npx cucumber-js",
    "detox:android-debug": "detox build --configuration android.emu.debug",
    "detox:e2e:android-debug": "DETOX_CONFIGURATION=android.emu.debug npx cucumber-js",
    "detox:android-release": "detox build --configuration android.emu.release",
    "detox:e2e:android-release": "DETOX_CONFIGURATION=android.emu.release npx cucumber-js"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "jest": {
    "preset": "react-native"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.18.2",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/stack": "^6.3.17",
    "axios": "^1.4.0",
    "i18next": "^23.2.3",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "plop": "^3.1.2",
    "react": "18.2.0",
    "react-i18next": "^13.0.1",
    "react-native": "0.72.0",
    "react-native-config": "^1.5.1",
    "react-native-device-info": "^10.6.0",
    "react-native-gesture-handler": "^2.12.0",
    "react-native-safe-area-context": "^4.6.3",
    "react-native-screens": "^3.22.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/register": "^7.22.5",
    "@babel/runtime": "^7.22.5",
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@cucumber/cucumber": "^9.3.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.6",
    "@tsconfig/react-native": "^3.0.2",
    "@types/jest": "^29.5.2",
    "@types/metro-config": "^0.76.3",
    "@types/react": "^18.2.13",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.5.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "detox": "^20.11.2",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^35.0.0",
    "eslint-import-resolver-babel-module": "^5.3.2",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^16.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.2",
    "metro-react-native-babel-preset": "0.76.7",
    "prettier": "^2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.1.3"
  },
  "engines": {
    "node": ">=16"
  }
}
